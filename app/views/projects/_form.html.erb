<%= nested_form_for @project do |f| %>
 <%= f.error_messages %>
  <fieldset>
    <div class="clearfix">
      <div class="input">
        <%= f.text_field :name ,:placeholder => 'Project Name'%>
      </div>
    </div>
  </fieldset>

  <%= f.fields_for :lists do |builder| %>
    <%= render "list", :f => builder %>
  <% end %>
  <p><%= f.link_to_add "Add a List", :lists,:class => "btn small" %></p>
  <p><%= f.submit "Submit",:class => "btn primary large"%></p>
<% end %>
